	"SCSS Module Class": {
		"prefix": "cls",
		"body": "className={style.$1}",
		"description": "Insert className with SCSS module style"
	},
	"Custom React Arrow Function with SCSS Module": {
		"prefix": "rs",
		"body": [
			"import React from 'react'",
			"import style from './${1:ComponentName}.module.scss'",
			"",
			"const ${1:ComponentName} = () => {",
			"  return (",
			"    <div className={style.main}></div>",
			"  )",
			"}",
			"",
			"export default ${1:ComponentName}"
		],
		"description": "Creates a React Arrow Function Component with SCSS module import"
	}
